<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">

<properties>
    <comment>SWModule encrypt with JKS.</comment>
    <!-- Key store configuration -->
    <entry key="1.keyStorePath">./keystore/eidasEncryptionKeyStore.p12</entry>
    <entry key="1.keyStorePassword">ENCRYPT_KEYSTORE_PASSWORD</entry>
    <entry key="1.keyPassword">ENCRYPT_KEYSTORE_KEY_PASSWORD</entry>
    <entry key="1.keyStoreType">PKCS12</entry>

    <!-- Key transport config -->
    <!--  If not present then no decryption will be applied on response -->
    <entry key="responseDecryptionIssuer">CN=eidas-local-encryption,OU=ID-porten,O=Digitaliseringsdirektoratet,L=Leikanger,ST=Sogndal,C=NO</entry>
    <entry key="serialNumber">4E372623D3C6BEEDF2814F5A1676C674BCF55D80</entry>

</properties>
