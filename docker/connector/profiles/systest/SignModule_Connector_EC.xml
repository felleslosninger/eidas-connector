<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">

<properties>
	<comment>SWModule sign with JKS.</comment>
	<!-- Request signature -->
	<entry key="1.keyStorePath">./keystore/otherCountriesEidasKeyStore.p12</entry>
	<entry key="1.keyStorePassword">TRUSTSTORE_PASSWORD</entry>
	<entry key="1.keyStoreType">PKCS12</entry>
	<entry key="1.keyStorePurpose">TRUSTSTORE</entry>


    <!-- Nist curve -->
	<entry key="issuer">CN=eidas-connector-test,OU=ID-porten,O=Digitaliseringsdirektoratet,C=NO</entry>
    <entry key="serialNumber">6B2BD49EAC4EDC335E95B51293DA01909191F003</entry>
    
    <!-- Configuration for Metadata signatures with EC -->
    <entry key="2.keyStorePath">./keystore/norwegianConnectorKeyStore_METADATA_EC.p12</entry>
	<entry key="2.keyStorePassword">SIGN_KEYSTORE_PASSWORD</entry>
	<entry key="2.keyPassword">SIGN_KEYSTORE_KEY_PASSWORD</entry>
	<entry key="2.keyStoreType">PKCS12</entry>
	<entry key="metadata.issuer">CN=eidas-connector-test,OU=ID-porten,O=Digitaliseringsdirektoratet,C=NO</entry>
    <entry key="metadata.serialNumber">6B2BD49EAC4EDC335E95B51293DA01909191F003</entry>



</properties>
